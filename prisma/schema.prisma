generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model BookingRequest {
  id        String   @id @default(cuid())
  name      String
  email     String
  cityVenue String?
  eventDate String?
  details   String?
  createdAt DateTime @default(now())
}