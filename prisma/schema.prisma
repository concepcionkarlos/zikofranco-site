generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  // Si tu Prisma está exigiendo usar prisma.config.ts, deja SIN url aquí.
  // Si NO te deja migrar sin url, entonces pon:
  // url = env("DATABASE_URL")
}

model BookingRequest {
  id        String   @id @default(cuid())
  name      String
  email     String
  cityVenue String?
  eventDate String?
  details   String?
  createdAt DateTime @default(now())
}